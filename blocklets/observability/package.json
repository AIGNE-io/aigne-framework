{
  "name": "@blocklet/observability",
  "version": "0.2.0",
  "description": "Blocklet Observability for tracing and observability",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/AIGNE-io/aigne-framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "scripts": {
    "lint": "tsc --noEmit && biome check",
    "lint:fix": "biome check --write && pnpm run lint",
    "clean": "node scripts/build-clean.mjs",
    "dev": "blocklet dev",
    "start": "cross-env NODE_ENV=development nodemon --exec tsx api/dev.ts -w api",
    "bundle": "tsc --noEmit && npm run bundle:client && npm run bundle:api",
    "bundle:client": "vite build",
    "bundle:api": "npm run clean && tsc -p tsconfig.api.json && blocklet bundle --compact --create-release --external @aigne/sqlite",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepare": "npx simple-git-hooks",
    "bump-version": "zx --quiet scripts/bump-version.mjs"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@biomejs/js-api": "^0.7.1",
    "@biomejs/wasm-nodejs": "^1.9.4",
    "@aigne/core": "workspace:^",
    "@aigne/observability": "workspace:^",
    "@aigne/openai": "workspace:^",
    "@aigne/sqlite": "workspace:^",
    "@aigne/transport": "workspace:^",
    "@arcblock/did-connect": "^2.13.70",
    "@arcblock/ux": "^2.13.70",
    "@blocklet/sdk": "^1.16.44",
    "@blocklet/ui-react": "^2.13.70",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@mui/material": "^5.15.19",
    "@mui/x-data-grid": "^8.5.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@types/dotenv-flow": "^3.3.3",
    "bumpp": "^10.2.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "express-sse": "^1.0.0",
    "flat": "^6.0.1",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "tinycolor2": "^1.6.0",
    "uuid": "^11.1.0",
    "vite-plugin-blocklet": "^0.9.33",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/lodash": "^4.17.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "zx": "^8.5.4"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
