{
  "name": "@aigne/observability",
  "version": "0.0.1",
  "description": "AIGNE Observability for tracing and observability",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/AIGNE-io/aigne-framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "files": [
    "frontend/dist",
    "lib/cjs",
    "lib/dts",
    "lib/esm",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "browser": "./lib/esm/index.browser.js",
      "node": {
        "require": "./lib/cjs/index.node.js",
        "import": "./lib/esm/index.node.js"
      },
      "default": "./lib/esm/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "index.browser": [
        "./lib/esm/index.browser.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "build": "run-p build:lib build:frontend",
    "build:lib": "tsc --build scripts/tsconfig.build.json",
    "build:frontend": "cd frontend && pnpm run build",
    "clean": "rimraf lib test/coverage",
    "prepublishOnly": "run-s clean build",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov --coverage-reporter=text",
    "postbuild": "echo '{\"type\": \"module\"}' > lib/esm/package.json && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json"
  },
  "dependencies": {
    "@aigne/sqlite": "workspace:^",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "express-sse": "^1.0.0",
    "flat": "^6.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  }
}
