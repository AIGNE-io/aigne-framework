{
  "name": "@aigne/memory",
  "version": "1.0.0",
  "description": "AIGNE Memory library",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/blocklet/ai-studio#readme",
  "license": "ISC",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.js",
  "bin": {
    "aigne-cli": "./lib/cjs/cmd/aigne-cli.js"
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "./*.js",
    "./*.cjs",
    "./*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocklet/ai-studio.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module es2022 --outDir lib/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "clean": "rimraf lib",
    "prepublishOnly": "run-s clean build"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "dependencies": {
    "@aigne/core": "workspace:^",
    "@blocklet/search-kit-js": "^0.2.23",
    "fs-extra": "^11.3.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.2",
    "sequelize": "^6.37.5",
    "snowflake-uuid": "^1.0.0",
    "sqlite3": "^5.1.7",
    "ufo": "^1.5.4",
    "umzug": "^3.8.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.14",
    "core-js": "^3.40.0",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  }
}
