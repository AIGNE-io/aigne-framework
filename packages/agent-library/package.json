{
  "name": "@aigne/agent-library",
  "version": "1.1.0-beta.17",
  "description": "Collection of agent libraries for AIGNE framework",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/AIGNE-io/aigne-framework/tree/main/packages/agent-library",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "files": ["lib/cjs", "lib/dts", "lib/esm", "LICENSE", "README.md", "CHANGELOG.md"],
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/dts/index.d.ts"
    }
  },
  "scripts": {
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "build": "tsc --build scripts/tsconfig.build.json",
    "clean": "rimraf lib coverage",
    "prepublishOnly": "run-s clean build",
    "test": "bun --env-file=.env.local test",
    "test:coverage": "bun test --coverage --coverage-reporter lcov"
  },
  "dependencies": {
    "@aigne/core-next": "workspace:^",
    "jsonata": "^2.0.6",
    "jsonschema": "^1.5.0",
    "to-json-schema": "^0.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.5",
    "@types/to-json-schema": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  }
}
