{
  "name": "@aigne/observability",
  "version": "0.0.1",
  "description": "AIGNE Observability for tracing and observability",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://github.com/AIGNE-io/aigne-framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "files": [
    "dist",
    "src",
    "lib/cjs",
    "lib/dts",
    "lib/esm",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "browser": "./lib/esm/index.browser.js",
      "node": {
        "require": "./lib/cjs/index.node.js",
        "import": "./lib/esm/index.node.js"
      },
      "default": "./lib/esm/index.js"
    },
    "./server": {
      "browser": "./lib/esm/server/index.js",
      "node": {
        "require": "./lib/cjs/server/index.js",
        "import": "./lib/esm/server/index.js"
      },
      "default": "./lib/esm/server/index.js"
    },
    "./list": {
      "default": "./src/list.tsx"
    },
    "./translations": {
      "default": "./src/locales/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index.browser": [
        "./lib/esm/index.browser.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "node scripts/build-clean.mjs",
    "lint": "tsc --noEmit && eslint .",
    "build": "run-p build:client build:api",
    "build:client": "tsc -b && vite build",
    "build:api": "tsc --build scripts/tsconfig.build.json",
    "prepublishOnly": "run-s clean build",
    "test": "bun test",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov --coverage-reporter=text",
    "postbuild": "echo '{\"type\": \"module\"}' > lib/esm/package.json && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json"
  },
  "dependencies": {
    "@aigne/sqlite": "workspace:^",
    "@arcblock/did-connect": "^2.13.70",
    "@arcblock/ux": "^2.13.70",
    "@blocklet/sdk": "^1.16.44",
    "@blocklet/ui-react": "^2.13.70",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@mui/material": "^5.15.19",
    "@mui/x-data-grid": "^8.5.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@types/dotenv-flow": "^3.3.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "express-sse": "^1.0.0",
    "flat": "^6.0.1",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "tinycolor2": "^1.6.0",
    "uuid": "^11.1.0",
    "vite-plugin-blocklet": "^0.9.33",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/lodash": "^4.17.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "zx": "^8.5.4"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
